{
    "output": "./output.cs",
    "generator": "angular",
    "mode": "ng-typescript",
    "transforms": {
        "serviceName": "pascalCase",
        "operationName": "camelCase",
        "modelName": "pascalCase",
        "propertyName": "camelCase"
    },
    "url": "http://rpos-dev.azurewebsites.net/swagger/docs/v1",
    "options": {
        "baseUrl": {
            "strategy": "ImportedVar",
            "importedVar": {
                "importFrom": "../../environments/environment",
                "importVariable": "environment",
                "property": "baseUri"
            },
            "property": "baseUri"
        },
        "customization": {
            "importFrom": "./customizations.ts",
            "importVariable": "ApiCustomizations"
        },
        "generation": {
            "generateInterfaces": true,
            "generateImplementations": true,
            "generateModels": true,
            "serviceClassSuffix": "Client",
            "modelClassSuffix": "Model"
        }
    }
}
