{
  "name": "swagen-angular",
  "version": "1.0.0-beta.27",
  "description": "Swagen generator for Angular",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "lint": "eslint ./**/*.js",
    "bump": "npm version prerelease --no-git-tag-version",
    "prepublishOnly": "npm run lint",
    "postpublish": "npm run bump",
    "update": "npm un --save swagen-typescript-language codewriter && npm i --save swagen-typescript-language codewriter",
    "test": "node ./test-harness"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swagen/swagen-angular.git"
  },
  "keywords": [
    "swagen",
    "angular"
  ],
  "author": "Swagen <swagen@jeevanjames.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/swagen/swagen-angular/issues"
  },
  "homepage": "https://github.com/swagen/swagen-angular#readme",
  "dependencies": {
    "codewriter": "^1.4.0-beta.1",
    "lodash": "^4.17.15",
    "swagen-typescript-language": "^1.1.0-beta.3"
  },
  "devDependencies": {
    "eslint": "^6.2.2"
  },
  "directories": {
    "lib": "lib"
  }
}
