{
    "name": "swagen-angular",
    "version": "1.0.0-beta.6",
    "description": "Swagen generator for Angular",
    "main": "index.js",
    "scripts": {
        "lint": "eslint lib/**/*.js",
        "bump": "npm version prerelease --no-git-tag-version",
        "prepublishOnly": "npm run lint",
        "postpublish": "npm run bump",
        "update": "npm un --save swagen-core swagen-typescript-language codewriter && npm i --save swagen-core swagen-typescript-language codewriter",
        "test": "node ./test-harness"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swagen/swagen-angular.git"
    },
    "keywords": [
        "swagen",
        "angular"
    ],
    "author": "Swagen <swagen@jeevanjames.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/swagen/swagen-angular/issues"
    },
    "homepage": "https://github.com/swagen/swagen-angular#readme",
    "dependencies": {
        "codewriter": "^1.2.0-beta.3",
        "lodash": "^4.17.4",
        "swagen-core": "^1.0.0-beta.2",
        "swagen-typescript-language": "0.0.3"
    },
    "devDependencies": {
        "eslint": "^4.7.2"
    }
}
